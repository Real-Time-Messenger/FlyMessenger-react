{
    "plugins": ["prettier-plugin-tailwindcss"],
    "tabWidth": 4,
    "trailingComma": "all",
    "semi": true,
    "useTabs": false,
    "printWidth": 120,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": "*.css",
            "options": {
                "singleQuote": false,
                "semi": false
            }
        }
    ]
}
